# vim: ft=sh sw=2 ts=2 expandtab

alias focus='open focus://focus'
alias focusfor='function _focusfor() { open focus://focus?minutes="$1"; };_focusfor'
alias breakfor='function _breakfor() { open focus://break?minutes="$1"; };_breakfor'
alias unfocus='open focus://unfocus'
alias unbreak='open focus://unbreak'
alias togglefocus='open focus://toggle'
alias focuspref='open focus://preferences'
alias focusdebug='open focus://debug'
